{
	"name": "warm-up-typing-test",
	"displayName": "Warm Up - Typing test",
	"publisher": "Jeusto",
	"description": "üî•üë®‚Äçüíª Improve your typing speed by practicing right inside your code editor.",
	"repository": {
		"type": "git",
		"url": "https://github.com/Jeusto/vscode-typing-test"
	},
	"icon": "icon.png",
	"version": "0.1.0",
	"license": "GPL 3.0",
	"keywords": [
		"arhun saday",
		"jeusto",
		"warm up",
		"warmup",
		"typing",
		"typing speed",
		"improve typing",
		"keyboard",
		"practice typing",
		"typing-test",
		"practice",
		"typing"
	],
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:warmUp.start",
		"onWebviewPanel:warmUp",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Warm Up: Practice typing",
				"properties": {
					"warmUp.switchLanguage": {
						"type": "string",
						"enum": [
							"english",
							"italian",
							"german",
							"spanish",
							"chinese",
							"korean",
							"polish",
							"punjabi",
							"swedish",
							"french",
							"portuguese",
							"russian",
							"finnish",
							"englishTop1000",
							"javascriptTemp"
						],
						"default": "english",
						"description": "Choose a specific language to practice with.",
						"scope": "window"
					}
				}
			},
			{
				"title": "Warm Up: Practice typing",
				"properties": {
					"warmUp.switchTypingMode": {
						"type": "string",
						"enum": [
							"wordcount",
							"time"
						],
						"default": "wordcount",
						"description": "Practice a set number of words or against a timer.",
						"scope": "window"
					}
				}
			},
			{
				"title": "Warm Up: Practice typing",
				"properties": {
					"warmUp.togglePunctuation": {
						"type": "string",
						"enum": [
							"true",
							"false"
						],
						"default": "false",
						"description": "Activate/deactivate punctuation.",
						"scope": "window"
					}
				}
			},
			{
				"title": "Warm Up: Practice typing",
				"properties": {
					"warmUp.changeCount": {
						"type": "string",
						"enum": [
							"15",
							"30",
							"60",
							"120",
							"240"
						],
						"default": "15",
						"description": "Change the amount of words or the timer (depending on the typing mode).",
						"scope": "window"
					}
				}
			}
		],
		"commands": [
			{
				"command": "warmUp.start",
				"title": "Start practicing typing",
				"category": "Warm Up"
			},
			{
				"command": "warmUp.switchLanguage",
				"title": "Switch language",
				"category": "Warm Up"
			},
			{
				"command": "warmUp.switchTypingMode",
				"title": "Switch typing mode",
				"category": "Warm Up"
			},
			{
				"command": "warmUp.changeCount",
				"title": "Change word/time count",
				"category": "Warm Up"
			},
			{
				"command": "warmUp.togglePunctuation",
				"title": "Toggle punctuation",
				"category": "Warm Up"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.47.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.3.5"
	}
}